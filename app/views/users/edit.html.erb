<h2>User info</h2>
<%= form_with model: @user do |f| %>
  <label for="inputName">Name</label>
  <%= f.text_field :name, autofocus: true, id:"inputName", placeholder:"Name"%>
  <label for="inputImage">ProfileImage</label>
  <%= f.file_field :profile_image, placeholder: "プロフィール画像", accept: "image/*" %>  
  <label for="Introduction">Introduction</label>
  <%= f.text_area :Introduction, id:"Introduction", placeholder:"Messege"%>
  <%= f.submit "Update User" %>
<% end %>